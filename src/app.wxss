/**app.wxss**/
page{
  color: black;
  font-size: 30rpx;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 头部样式 */
.header{
  width: 750rpx;
  line-height: 96rpx;
  background: green;
  padding-top: 40rpx;
  text-align: center;
}
.header image{
  display: flex;
  margin: 10rpx 18rpx;
  width: 160rpx;
  height: 64rpx;
  position: absolute;
  left: 0;
} 
.header-title{
font-size: 35rpx;
color: white;
}
